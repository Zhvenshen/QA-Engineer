
import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
// т.к в задании не было указано, какого типа должна быть строка, я выбрала тип String
        Scanner scanner = new Scanner(System.in);
        System.out.print ("Введите строку 'a' :");
        String a = scanner.nextLine();
        System.out.print ("Введите строку 'b' :");
        String b = scanner.nextLine();
        if (a==b) System.out.print("Строки идентичны");
        else System.out.print("Строки неидентичны");
    }
}
